{
  "name": "gitlab-pipeline-deleter",
  "version": "2.2.57",
  "description": "A CLI tool that deletes old GitLab pipelines",
  "main": "target/src/index.js",
  "bin": {
    "glpd": "target/src/bin.js"
  },
  "author": "Christian Rackerseder <git@echooff.de>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/screendriver/gitlab-pipeline-deleter.git"
  },
  "bugs": {
    "url": "https://github.com/screendriver/gitlab-pipeline-deleter/issues"
  },
  "homepage": "https://github.com/screendriver/gitlab-pipeline-deleter#readme",
  "keywords": [
    "gitlab",
    "pipeline",
    "ci",
    "clean",
    "delete",
    "remove"
  ],
  "files": [
    "target/src/**/*.js",
    "target/src/**/*.js.map"
  ],
  "scripts": {
    "compile": "tsc --build",
    "lint": "eslint src test --ext '.ts,.tsx'",
    "copy:paste:detection": "jscpd src test",
    "test:unit": "FORCE_COLOR=1 TS_NODE_PROJECT=tsconfig.base.json mocha 'test/unit/**/*.test.*'",
    "test:unit:coverage": "c8 npm run test:unit",
    "test:integration": "TS_NODE_PROJECT=tsconfig.base.json mocha --timeout 30000 'test/integration/**/*.test.*'",
    "test:unit:mutation": "stryker run"
  },
  "dependencies": {
    "commander": "8.2.0",
    "cosmiconfig": "7.0.1",
    "date-fns": "2.24.0",
    "got": "11.8.2",
    "ink": "3.0.9",
    "ink-spinner": "4.0.2",
    "p-queue": "6.6.2",
    "react": "17.0.2",
    "true-myth": "4.1.1",
    "tslib": "2.3.1",
    "urlcat": "2.0.4",
    "zod": "3.8.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.1",
    "@semantic-release/github": "7.2.3",
    "@semantic-release/npm": "7.1.3",
    "@semantic-release/release-notes-generator": "9.0.3",
    "@stryker-mutator/core": "5.4.0",
    "@stryker-mutator/typescript-checker": "5.4.0",
    "@types/chai": "4.2.22",
    "@types/common-tags": "1.8.1",
    "@types/micro": "7.3.6",
    "@types/microrouter": "3.1.1",
    "@types/mocha": "9.0.0",
    "@types/react": "17.0.22",
    "@types/sinon": "10.0.3",
    "@types/test-listen": "1.1.0",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "c8": "7.9.0",
    "chai": "4.3.4",
    "common-tags": "1.8.0",
    "cooky-cutter": "1.5.4",
    "delay": "5.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-mocha": "9.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.26.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "4.12.3",
    "ink-testing-library": "2.1.0",
    "jscpd": "3.3.26",
    "micro": "9.3.4",
    "microrouter": "3.1.3",
    "mocha": "9.1.1",
    "prettier": "2.4.1",
    "semantic-release": "17.4.7",
    "sinon": "11.1.2",
    "test-listen": "1.1.0",
    "ts-node": "10.2.1",
    "typescript": "4.4.3"
  }
}
