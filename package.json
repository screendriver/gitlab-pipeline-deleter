{
    "name": "gitlab-pipeline-deleter",
    "version": "2.3.13",
    "description": "A CLI tool that deletes old GitLab pipelines",
    "main": "target/src/index.js",
    "bin": {
        "glpd": "target/src/bin.js"
    },
    "author": "Christian Rackerseder <git@echooff.de>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/screendriver/gitlab-pipeline-deleter.git"
    },
    "bugs": {
        "url": "https://github.com/screendriver/gitlab-pipeline-deleter/issues"
    },
    "homepage": "https://github.com/screendriver/gitlab-pipeline-deleter#readme",
    "keywords": [
        "gitlab",
        "pipeline",
        "ci",
        "clean",
        "delete",
        "remove"
    ],
    "files": [
        "target/src/**/*.js",
        "target/src/**/*.js.map"
    ],
    "scripts": {
        "compile": "tsc --build",
        "lint": "eslint src test --ext '.ts,.tsx'",
        "copy:paste:detection": "jscpd src test",
        "test:unit": "TS_NODE_PROJECT=tsconfig.base.json uvu -r ts-node/register/transpile-only 'test/unit/' '.*\\.test\\.(ts|tsx)$'",
        "test:unit:coverage": "c8 npm run test:unit",
        "test:integration": "TS_NODE_PROJECT=tsconfig.base.json uvu -r ts-node/register/transpile-only 'test/integration/' '.*\\.test\\.(ts|tsx)$'",
        "test:unit:mutation": "stryker run"
    },
    "dependencies": {
        "@sindresorhus/is": "4.6.0",
        "commander": "9.4.1",
        "cosmiconfig": "7.1.0",
        "date-fns": "2.29.3",
        "got": "11.8.6",
        "ink": "3.2.0",
        "ink-spinner": "4.0.3",
        "p-queue": "6.6.2",
        "react": "17.0.2",
        "true-myth": "4.1.1",
        "tslib": "2.4.1",
        "urlcat": "2.0.4",
        "zod": "3.19.1"
    },
    "devDependencies": {
        "@semantic-release/changelog": "6.0.2",
        "@semantic-release/commit-analyzer": "9.0.2",
        "@semantic-release/git": "10.0.1",
        "@semantic-release/github": "8.0.7",
        "@semantic-release/npm": "9.0.1",
        "@semantic-release/release-notes-generator": "10.0.3",
        "@stryker-mutator/core": "6.3.0",
        "@stryker-mutator/typescript-checker": "6.3.0",
        "@swc/core": "1.3.22",
        "@swc/helpers": "0.4.14",
        "@types/common-tags": "1.8.1",
        "@types/micro": "7.3.7",
        "@types/microrouter": "3.1.2",
        "@types/react": "17.0.52",
        "@types/sinon": "10.0.13",
        "@types/test-listen": "1.1.0",
        "@typescript-eslint/eslint-plugin": "5.44.0",
        "@typescript-eslint/parser": "5.44.0",
        "c8": "7.12.0",
        "delay": "5.0.0",
        "eslint": "8.28.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.31.11",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-testing-library": "5.9.1",
        "fishery": "2.2.2",
        "ink-testing-library": "2.1.0",
        "jscpd": "3.5.1",
        "micro": "9.3.4",
        "microrouter": "3.1.3",
        "prettier": "2.7.1",
        "semantic-release": "19.0.5",
        "sinon": "14.0.2",
        "test-listen": "1.1.0",
        "ts-node": "10.9.1",
        "typescript": "4.9.4",
        "uvu": "0.5.6"
    }
}
