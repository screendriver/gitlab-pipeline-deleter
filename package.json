        "@types/common-tags": "1.8.1",
        "@types/micro": "7.3.7",
        "@types/microrouter": "3.1.2",
        "@types/react": "17.0.47",
        "@types/sinon": "10.0.11",
        "@types/test-listen": "1.1.0",
        "@typescript-eslint/eslint-plugin": "5.30.0",
        "@typescript-eslint/parser": "5.30.0",
        "ava": "4.3.0",
        "c8": "7.11.3",
        "common-tags": "1.8.2",
        "delay": "5.0.0",
        "eslint": "8.18.0",
