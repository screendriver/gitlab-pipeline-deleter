        "micro": "9.3.4",
        "microrouter": "3.1.3",
        "prettier": "2.7.1",
        "semantic-release": "19.0.3",
        "sinon": "14.0.0",
        "test-listen": "1.1.0",
        "ts-node": "10.8.1",
        "typescript": "4.7.4"
