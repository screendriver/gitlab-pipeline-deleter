{
    "name": "gitlab-pipeline-deleter",
    "version": "2.3.18",
    "description": "A CLI tool that deletes old GitLab pipelines",
    "main": "target/src/index.js",
    "bin": {
        "glpd": "target/src/bin.js"
    },
    "author": "Christian Rackerseder <git@echooff.de>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/screendriver/gitlab-pipeline-deleter.git"
    },
    "bugs": {
        "url": "https://github.com/screendriver/gitlab-pipeline-deleter/issues"
    },
    "homepage": "https://github.com/screendriver/gitlab-pipeline-deleter#readme",
    "keywords": [
        "gitlab",
        "pipeline",
        "ci",
        "clean",
        "delete",
        "remove"
    ],
    "files": [
        "target/src/**/*.js",
        "target/src/**/*.js.map"
    ],
    "scripts": {
        "compile": "tsc --build",
        "lint": "eslint src test --ext '.ts,.tsx'",
        "copy:paste:detection": "jscpd src test",
        "test:unit": "TS_NODE_PROJECT=tsconfig.base.json ava",
        "test:unit:coverage": "c8 npm run test:unit",
        "test:integration": "TS_NODE_PROJECT=tsconfig.base.json ava --config ava.config.integration.mjs",
        "release:dry": "release-it --dry-run --no-npm",
        "release": "release-it"
    },
    "dependencies": {
        "@sindresorhus/is": "4.6.0",
        "commander": "9.5.0",
        "cosmiconfig": "7.1.0",
        "date-fns": "2.30.0",
        "got": "11.8.6",
        "ink": "3.2.0",
        "ink-spinner": "4.0.3",
        "p-queue": "6.6.2",
        "react": "17.0.2",
        "true-myth": "4.1.1",
        "tslib": "2.6.0",
        "urlcat": "3.1.0",
        "zod": "3.21.4"
    },
    "devDependencies": {
        "@swc/core": "1.3.69",
        "@swc/helpers": "0.5.1",
        "@types/common-tags": "1.8.1",
        "@types/micro": "7.3.7",
        "@types/microrouter": "3.1.3",
        "@types/react": "17.0.62",
        "@types/sinon": "10.0.15",
        "@types/test-listen": "1.1.0",
        "@typescript-eslint/eslint-plugin": "5.62.0",
        "@typescript-eslint/parser": "5.62.0",
        "ava": "5.3.1",
        "c8": "7.14.0",
        "delay": "5.0.0",
        "eslint": "8.45.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-plugin-ava": "14.0.0",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-testing-library": "5.11.0",
        "fishery": "2.2.2",
        "ink-testing-library": "2.1.0",
        "jscpd": "3.5.9",
        "micro": "9.3.4",
        "microrouter": "3.1.3",
        "prettier": "2.8.8",
        "release-it": "16.1.2",
        "sinon": "14.0.2",
        "test-listen": "1.1.0",
        "ts-node": "10.9.1",
        "typescript": "4.9.5"
    }
}
