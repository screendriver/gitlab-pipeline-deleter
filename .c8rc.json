{
  "all": true,
  "lines": 100,
  "functions": 100,
  "statements": 100,
  "branches": 100,
  "instrument": true,
  "extension": [".ts", ".tsx"],
  "include": ["src/**/*"],
  "exclude": ["src/network.ts"],
  "reporter": ["lcov", "text-summary", "json"],
  "check-coverage": true,
  "reportDir": "./target/coverage",
  "tempDirectory": "./target/c8_output"
}
